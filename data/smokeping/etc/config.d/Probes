*** Probes ***

+ FPing
binary = /usr/bin/fping
pings = 5
timeout = 1
step = 60

+ FPing6
binary = /usr/bin/fping
protocol = 6

+ DNS
binary = /usr/bin/dig
lookup = google.com
pings = 5
step = 300

+ TCPPing
binary = /usr/bin/tcpping
forks = 10
offset = random
pings = 5
port = 80

+ Curl

binary = /usr/bin/curl
offset = random
pings = 5
urlformat = http://%host%/
